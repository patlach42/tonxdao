{
  "name": "telegramapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-server": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-client": "openapi-ts --input ./openapi.json --output ./src/client --client axios --exportSchemas true",
    "get-openapi": "rm openapi.json; wget https://white.tailc4a5e.ts.net/api/v1/openapi.json",
    "update-openapi": "npm run get-openapi && npm run generate-client"
  },
  "dependencies": {
    "@react-three/drei": "^9.105.6",
    "@react-three/fiber": "^8.16.6",
    "@react-three/postprocessing": "^2.16.2",
    "@react-three/rapier": "^1.3.1",
    "@twa-dev/sdk": "^7.0.0",
    "@types/three": "^0.164.1",
    "axios": "^1.7.2",
    "babel-plugin-glsl": "^1.0.0",
    "centrifuge": "^5.2.0-beta.1",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "three": "^0.164.1",
    "vite-plugin-glsl": "^1.3.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.34.1",
    "@types/react": "^18.2.66",
